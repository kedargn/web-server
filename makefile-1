tcp_client: tcp_client.c 
	gcc tcp_client.c -o tcp_client.out 

tcp_server: tcp_server.c
	gcc tcp_server.c -o tcp_server.out

udp_client: udp_client.c
	gcc udp_client.c -o udp_client.out

udp_server: udp_server.c
	gcc udp_server.c -o udp_server.c


tcp_thread_server: tcp_thread_server.c
	gcc tcp_thread_server.c -pthread -o tcp_thread_server.out
